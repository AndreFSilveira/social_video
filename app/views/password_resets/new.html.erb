<% provide(:title, "Forgot password") %>
<h1>Forgot password</h1>

<div class="row">
  <div class="col m6 offset-m3 s12">
    <%= simple_form_for :password_reset, url: password_resets_path do |f| %>

        <%= f.input :email, wrapper: :materialize_input %>

        <%= f.button :button, wrapper: :materialize_button %>
    <% end %>
  </div>
</div>