<li><%= link_to t("header.links.video"), videos_path %></li>
<li><%= link_to t("header.links.news"), news_index_path %></li>
<% if logged_in? %>
    <li><%= link_to t("header.links.users"), users_path %></li>
    <li class="dropdown">
    <li><%= link_to t("header.links.friends"), user_friendship_path %></li>
    <li><%= link_to t("header.links.tag"), tags_path %></li>
    <li>
      <%= link_to "Log out", logout_path, method: "delete" %>
    </li>
<% else %>
    <li>
      <%= link_to t("header.links.register"),new_user_path %>
    </li>
    <li>
      <%= link_to login_path do %>
          <%= t("header.links.login") %>
          <i class="material-icons right">account_circle</i>
      <% end %>
    </li>

<% end %>
<li class="space"></li>
<li class="translate-icons"><%= link_to_unless_current image_tag("pt_br.png", alt: "Português"), locale: 'pt-BR' %></li>
<li class="translate-icons"><%= link_to_unless_current image_tag("es.png", alt: "Español"), locale: 'es' %></li>